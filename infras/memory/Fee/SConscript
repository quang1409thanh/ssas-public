from building import *

generate(Glob('*.json'))

CWD = GetCurrentDir()
objs = Glob('*.c') + Glob('GEN/*.c')

@register_library
class LibraryFee(Library):
    def config(self):
        self.include = CWD
        self.LIBS = ['Crc', 'Factory']
        self.CPPPATH = ['$INFRAS']
        self.source = objs

    
